#Task_12
print("Task_12") 
number = int(input("Entre Any Big Number: "))
num = str(number)
n = list(num)
print("list: ",n)
sum = 0
for i in n:
   s = str(i)
   n = int(s)
   sum += n
print("int: ", sum)

