for i in lst:
    i+=10
    print(i)
  
#Task_7
print("Task_7")  
import random
lst = []
for l in range(10):
    l = str(random.randint(25,110))
    lst.append(l)
print("List: ",lst)
print("Min Number: ",min(lst))


