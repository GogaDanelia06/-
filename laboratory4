#Task1print("Task_1")numbers = [5,12,52,4,9]sum = 0for i in numbers:    sum+=iprint("Num Sum: ",sum)maximum = max(numbers)minimum = min(numbers)avg = sum/len(numbers)print("Max Number: ",maximum)print("Min Number: ",minimum)print("Avg : ",avg)numbers.append(102)print("102 Added To Last: ",numbers)numbers.insert(2,205)print("205 Third Position: ",numbers)numbers.pop(3)print("Delete From Fourth: ",numbers)numbers.sort()print("Increasing: ",numbers)#Task_2print("Task_2")l = []for i in range(10):    i = input("Enter 10 Number: ")    l.append(i)print("10 Number: ",l)#Task_3print("Task_3")fruits = ["Watermelon", "Banana", "Apple"]fruits.sort()fruits.reverse()print(fruits)#Task_4print("Task_4")def li(l):  m = 1  for i in l:    m=m * i  return mlst = [5,6,7,3,9]print(li(lst))#Task_5print("Task_5")def remove_even(l):    for i in l:        if i % 2 == 0:            l.remove(i)    print (l)remove_even([4,5,4])#Task_6print("Task_6")lst = [6,12,51,37,9]for i in lst:    i+=10    print(i)  #Task_7print("Task_7")  import randomlst = []for l in range(10):    l = str(random.randint(25,110))    lst.append(l)print("List: ",lst)print("Min Number: ",min(lst))#Task_8print("Task_8")  #Task_9print("Task_9")  def li(l):    for i in l:        if i%2!=0:            l.remove(i)    print(l)lst = [61,32,6,9,10,41]li(lst)#Task_10print("Task_10")  lst = [61,32,6,9,10,41]random.shuffle(lst)print(lst)#Task_11print("Task_11") lst = [61,32,6,9,10,41]lst = random.choice(lst)print(lst)#Task_12print("Task_12") number = int(input("Entre Any Big Number: "))num = str(number)n = list(num)print("list: ",n)sum = 0for i in n:   s = str(i)   n = int(s)   sum += nprint("int: ", sum)#Task_13print("Task_13") a_list = [1, 3, 2]a_set = set(a_list)contains_duplicates = len(a_list) != len(a_set)print(contains_duplicates)#Task_14print("Task_14") f = input("Enter File Name: ")extensions = ['txt', 'jpg', 'gif', 'html']if f in extensions:   print("Yes")else:   print("No")#Task_15print("Task_15") 