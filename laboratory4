#Task1
print("Task_1")
numbers = [5,12,52,4,9]
sum = 0
for i in numbers:
    sum+=i
print("Num Sum: ",sum)
maximum = max(numbers)
minimum = min(numbers)
avg = sum/len(numbers)
print("Max Number: ",maximum)
print("Min Number: ",minimum)
print("Avg : ",avg)
numbers.append(102)
print("102 Added To Last: ",numbers)
numbers.insert(2,205)
print("205 Third Position: ",numbers)
numbers.pop(3)
print("Delete From Fourth: ",numbers)
numbers.sort()
print("Increasing: ",numbers)

#Task_2
print("Task_2")
l = []
for i in range(10):
    i = input("Enter 10 Number: ")
    l.append(i)
print("10 Number: ",l)

#Task_3
print('Task_3')
thislist = ["watemelon", "banana", "apple"]

for i in reversed(thislist):
    print(i)

#Task_4
print("Task_4")
def li(l):
  m = 1
  for i in l:
    m=m * i
  return m
lst = [5,6,7,3,9]
print(li(lst))

#Task_5
print("Task_5")
def remove_even(l):
    for i in l:
        if i % 2 == 0:
            l.remove(i)
    print (l)
remove_even([4,5,4])

